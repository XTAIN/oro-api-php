{
  "name": "oro/oro-api-php",
  "description": "Oro API client library for PHP.",
  "keywords": [
    "oro"
  ],
  "homepage": "https://www.mollie.com/en/developers",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "digital print Laserdruck Centrum Garbsen GmbH",
      "email": "technik@digital-print.net"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "composer/ca-bundle": "^1.2"
  },
  "require-dev": {
    "eloquent/liberator": "^2.0",
    "guzzlehttp/guzzle": "^6.3 || ^7.0",
    "phpunit/phpunit": "^5.7 || ^6.5 || ^7.1 || ^8.5 || ^9.5",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Oro\\Api\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Tests\\Oro\\Api\\": "tests/Oro/API/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit tests",
    "format": "./vendor/bin/php-cs-fixer fix --allow-risky=yes"
  }
}
